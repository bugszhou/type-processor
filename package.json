{
    "name": "type-processor",
    "version": "0.1.0",
    "description": "use typescript develop lib",
    "author": "yiri.zhou",
    "main": "dist/type-processor.min.js",
    "module": "dist/type-processor.es.js",
    "types": "typings/index.d.ts",
    "scripts": {
        "lint": "eslint --ext .ts --ext .tsx src -c .eslintrc.js",
        "fix": "npm run lint --fix",
        "prebuild": "rimraf dist",
        "dev": "rollup -c rollup.config.ts -w",
        "build": "tsc --module commonjs && rollup -c rollup.config.ts",
        "prepublish": "npm run build",
        "commit": "git-cz --disable-emoji",
        "prepare": "rimraf .husky && npm run husky",
        "husky": "husky install && npm run commit-msg && npm run pre-push",
        "commit-msg": "husky add .husky/commit-msg \"npx --no-install commitlint --edit $1\"",
        "pre-push": "husky add .husky/pre-push \"npm run test\"",
        "unhusky": "husky uninstall"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bugszhou/type-processor"
    },
    "keywords": [
        "type-processor"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bugszhou/type-processor/issues"
    },
    "homepage": "https://github.com/bugszhou/type-processor",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-eslint": "^8.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.1.1",
        "@types/jest": "^26.0.23",
        "@types/node": "^14.14.25",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "commitlint": "^11.0.0",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "git-cz": "^4.7.6",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "lodash.camelcase": "^4.3.0",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.38.5",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.29.0",
        "ts-jest": "^26.5.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@types/node": "^15.12.2"
    }
}
